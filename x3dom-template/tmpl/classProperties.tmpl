<?js
    var props = obj;
?>

<div>
    <?js
         if (props && props.length )
         {
    ?>
    <div class="row">
        <div>Access</div>
        <div>Name</div>
        <div>Type</div>
        <div>Inheritance</div>
        <div>Description</div>
    </div>

    <?js props.forEach(function(p){ ?>
    <div class="row">
        <div><?js= p.access ?></div>
        <div><?js= p.name ?></div>
        <div>
            <?js=
                    ( p.type && p.type.names && p.type.names.length ) ? p.type.names[0] : "Not found"
            ?>
        </div>
        <div>
            <?js if(p.inherits)
                     {
                        var inherit = p.inherits.split('#');
                        inherit = inherit[inherit.length-2].split('.');
                        if(inherit.length)
                        {
                ?>
            <?js= inherit[inherit.length-1] ?>
            <?js }} ?>
        </div>
        <div><?js= p.description ?></div>
    </div>
    <?js }) ?>
    <?js }   ?>
</div>